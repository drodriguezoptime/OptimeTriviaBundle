{
  "name": "optime/trivia-bundle",
  "type": "symfony-bundle",
  "description": "Symfony OptimeTriviaBundle",
  "keywords": [
    "trivia",
    "optime"
  ],
  "homepage": "https://optimeconsulting.com",
  "license": "MIT",
  "authors": [
    {
      "name": "David Rodriguez",
      "email": "drodriguez@optimeconsulting.com",
      "homepage": "https://optimeconsulting.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "sonata-project/easy-extends-bundle": "^2.5",
    "symfony/config": "^2.8 || ^3.2 || ^4.0",
    "symfony/console": "^2.8 || ^3.2 || ^4.0",
    "symfony/dependency-injection": "^2.8 || ^3.2 || ^4.0",
    "symfony/form": "^2.8 || ^3.2 || ^4.0",
    "symfony/framework-bundle": "^2.8 || ^3.2 || ^4.0",
    "symfony/http-foundation": "^2.8 || ^3.2 || ^4.0",
    "symfony/http-kernel": "^2.8 || ^3.2 || ^4.0",
    "symfony/options-resolver": "^2.8 || ^3.2 || ^4.0",
    "symfony/routing": "^2.8 || ^3.2 || ^4.0",
    "symfony/security-core": "^2.8 || ^3.2 || ^4.0",
    "symfony/templating": "^2.8 || ^3.2 || ^4.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.4",
    "symfony/phpunit-bridge": "^4.2"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Optime\\TriviaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Optime\\TriviaBundle\\Tests\\": "tests/"
    }
  }
}
